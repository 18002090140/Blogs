<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.deng.customer.mapper.LifeImageMapperExt" >

    <select id="getImages" resultType="com.deng.customer.web.result.LifeImageResult">
        select id,url,sizes,datatime,title
        from life_image
        ORDER BY datatime DESC
    </select>

</mapper>